{
  "name": "vee-validate-legacy",
  "description": "Template Based Validation Framework for Vue.js",
  "version": "1.0.0",
  "author": "Vinccool96 <Vinccool96@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Vinccool96/vee-validate-legacy.git"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "pnpm build",
    "lint": "eslint . --fix",
    "checkTypes": "tsc src/*.ts test/*.ts --noEmit --esModuleInterop --resolveJsonModule"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./index.full": {
      "import": "./dist/index.full.mjs",
      "types": "./dist/index.full.d.ts",
      "require": "./dist/index.full.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs"
    },
    "./locale/*.json": "./locale/*.json"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "sync-hookable": "^1.0.1",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jsdom": "^21.1.4",
    "@types/node": "^18.18.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "flush-promises": "^1.0.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "raf-stub": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "resolutions": {
    "v-click-outside-vue3": "workspace:*"
  }
}
