{
  "name": "vee-validate-legacy",
  "description": "Template Based Validation Framework for Vue.js",
  "version": "1.0.0",
  "author": "Vinccool96 <Vinccool96@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Vinccool96/vee-validate-legacy.git"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "yarn build",
    "lint": "eslint . --fix",
    "checkTypes": "tsc src/*.ts test/*.ts --noEmit --esModuleInterop --resolveJsonModule"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./index.full": {
      "import": "./dist/index.full.mjs",
      "types": "./dist/index.full.d.ts",
      "require": "./dist/index.full.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs"
    },
    "./locale/*.json": "./locale/*.json"
  },
  "files": [
    "dist",
    "locale"
  ],
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "sync-hookable": "^1.0.1",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "raf-stub": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  }
}
